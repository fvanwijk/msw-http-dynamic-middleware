  "dependencies": {
    "pino": "6.13.0",
    "pino-pretty": "5.1.2"
  },
  "devDependencies": {
    "@types/pino": "6.3.11",
    "ts-node": "10.1.0",
    "tsdx": "0.14.1",
    "tslib": "1.14.1",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@mswjs/http-middleware": ">=0.1.1",
    "msw": ">=0.25"
  }
