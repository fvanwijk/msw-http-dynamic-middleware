{
  "name": "msw-dynamic-http-middleware",
  "version": "0.6.0",
  "description": "Create dynamic handlers for MSW HTTP middleware",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.tsdx.json",
    "build": "tsdx build --tsconfig tsconfig.tsdx.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "server": "node --loader ts-node/esm --es-module-specifier-resolution=node server.ts"
  },
  "type": "module",
  "module": "dist/msw-dynamic-http-middleware.esm.js",
  "keywords": [
    "msw",
    "http-middleware",
    "rest",
    "mocks"
  ],
  "author": "Frank van Wijk <info@frankvanwijk.nl>",
  "repository": {
    "type": "git",
    "url": "https://github.com/fvanwijk/msw-http-dynamic-middleware.git"
  },
  "license": "MIT",
  "prettier": {
    "arrowParens": "avoid",
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all"
  },
  "dependencies": {
    "@types/jest": "26.0.24",
    "pino": "6.13.0",
    "pino-pretty": "5.1.2"
  },
  "devDependencies": {
    "@mswjs/http-middleware": "0.1.1",
    "@types/pino": "6.3.11",
    "msw": "0.33.1",
    "prettier": "1.19.1",
    "ts-node": "10.1.0",
    "tsdx": "0.14.1",
    "tslib": "1.14.1",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@mswjs/http-middleware": ">=0.1.1",
    "msw": ">=0.25"
  }
}
