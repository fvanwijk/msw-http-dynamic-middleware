{
  "name": "msw-dynamic-http-middleware",
  "version": "0.5.0",
  "description": "Create dynamic handlers for MSW HTTP middleware",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.tsdx.json",
